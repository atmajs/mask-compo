UTest({
	'global hotkeys' (done) {
		UTest
			.server
			.request('/examples/keyboard/hotkeys.html')
			.done(function(doc, win){
				
				UTest
					.domtest(doc.body, `
							
						do press('alt+e');
						log('some');
						find(.current) > has ('text', '- alt+e');
						
						//do press('b');
						//find(.current) > has ('text', '- b');
						
						//do press('c+d');
						//find(.current) > has ('text', 'c+d');
					`)
					.always(done);
			});
	}
})